doctype html
html.mdw-app.mdw-theme(lang='en'
  mdw-fill="white" mdw-light mdw-theme="colored"
  mdw-sidesheet-toggle="dismissible" mdw-sidesheet-style="clipped")
  head
    title MDW Web App
    meta(charset='utf-8')
    meta(content='IE=edge', http-equiv='X-UA-Compatible')
    meta(content='MDW Web App', name='description')
    meta(content="width=device-width, initial-scale=1, viewport-fit=cover", name='viewport')
    meta(name="theme-color" content="#fff")
    meta(name="apple-mobile-web-app-status-bar-style" content="black-translucent")
    meta(name="apple-mobile-web-app-capable" content="yes")
    link(rel='stylesheet' href="//fonts.googleapis.com/icon?family=Material+Icons")
    link(rel='stylesheet' href='//fonts.googleapis.com/css?family=Roboto:300,400,500,700')
    link(rel='stylesheet' href='docs.min.css')
    link(rel='stylesheet' href='components.min.css')
    
    link(rel='stylesheet' href='default-theme.min.css')
    link(rel='stylesheet' href='alt-themes.min.css')

  body.mdw-app__body
    .mdw-app__menus
      include ./_menus.pug
    .mdw-app__dialogs
      include ./_dialogs.pug
    .mdw-app__navdrawer
      .mdw-app__sheet-content
        .mdw-list
          li.mdw-list__item Attractions
            .mdw-list__icon.material-icons local_activity
          li.mdw-list__item Dining
            .mdw-list__icon.material-icons local_dining
          li.mdw-list__item Education
            .mdw-list__icon.material-icons edit
          li.mdw-list__item Family
            .mdw-list__icon.material-icons people
          li.mdw-list__item Health
            .mdw-list__icon.material-icons healing
          li.mdw-list__item Office
            .mdw-list__icon.material-icons business
          li.mdw-list__item Promotions
            .mdw-list__icon.material-icons local_offer
          li.mdw-list__item Radio
            .mdw-list__icon.material-icons radio
          li.mdw-list__item Recipes
            .mdw-list__icon.material-icons book
          li.mdw-list__item Sports
            .mdw-list__icon.material-icons stars
          li.mdw-list__item Travel
            .mdw-list__icon.material-icons local_airport
    .mdw-app__sidesheet(mdw-toggle="dismissible" mdw-style="clipped")
      .mdw-app__sheet-content
        div
          span
            span.mdw-type(mdw-style="h2") Info
            .mdw-button(mdw-icon).material-icons.mdw-app__sidesheet-hide close
    .mdw-app__scrim
    .mdw-app__appbar(mdw-autohide="mobile tablet")
      .mdw-app__appbar-shape
        .mdw-app__fab-cut-track
          svg
            mask#fabMask(maskContentUnits='objectBoundingBox', x='0', y='0', width='1', height='1')
              rect(fill='white', x='0', y='0', width='1', height='1')
              ellipse(cx='0.5', cy='0', rx='.5', ry='1', fill='black', style='transform-origin:0.5px 0')
            rect(x='0', y='36', width='100%', height='100%')
            rect(x='0', y='0', width='72', height='36', mask='url(#fabMask)')
      .mdw-toolbar
        .mdw-toolbar__action
          .mdw-toolbar__start
            .mdw-button(mdw-icon).material-icons.mdw-app__navdrawer-toggle menu
          .mdw-toolbar__title MDW Web App
          .mdw-toolbar__end
            .mdw-button(mdw-icon).material-icons.mdw-app__sidesheet-toggle info
      .mdw-app__overlay-anchor
        .mdw-app__overlay
    .mdw-app__banner
      .mdw-banner
        .mdw-banner__container
          .mdw-banner__icon
            i.material-icons(style="font-size:40px") report_problem
          .mdw-banner__text
            | Your internet connection was lost.
            | We can't upload your photos.
          .mdw-banner__buttons
            .mdw-button Fix it
            .mdw-button Learn more
    // .mdw-app__bottomnav
      .mdw-bottomnav.mdw-theme(mdw-color="primary")
        .mdw-bottomnav__item(mdw-badge)
          .mdw-bottomnav__icon.material-icons history
          .mdw-bottomnav__label Recents
        .mdw-bottomnav__item(mdw-selected)
          .mdw-bottomnav__icon.material-icons favorite
          .mdw-bottomnav__label Favorites
        .mdw-bottomnav__item(mdw-badge="12")
          .mdw-bottomnav__icon.material-icons near_me
          .mdw-bottomnav__label Nearby
    .mdw-app__fab(mdw-mobile mdw-desktop aria-hidden="false")
      .mdw-app__fab-anchor
        .mdw-fab
          .mdw-fab__button.mdw-button.mdw-theme(mdw-fill="accent" mdw-dark)
            .mdw-fab__icon
              .mdw-fab__hidden-icon.material-icons add
              .mdw-fab__shown-icon.material-icons clear
            .mdw-fab__text Add
          .mdw-fab__speed-dial
            .mdw-button.mdw-fab__button(mdw-mini)
              .mdw-fab__icon.material-icons filter_1
            .mdw-button.mdw-fab__button(mdw-mini)
              .mdw-fab__icon.material-icons filter_2
            .mdw-button.mdw-fab__button(mdw-mini)
              .mdw-fab__icon.material-icons filter_3
            .mdw-button.mdw-fab__button(mdw-mini)
              .mdw-fab__icon.material-icons filter_4
            .mdw-button.mdw-fab__button(mdw-mini)
              .mdw-fab__icon.material-icons filter_5
    .mdw-app__snackbar
      .mdw-app__snackbar-anchor
        .mdw-snackbar__container
          .mdw-snackbar(aria-hidden="false")
            span Extremely long snackbar action text. This should text-wrap automatically when it's too large
            button.mdw-button.mdw-theme(mdw-color="accent") Action
