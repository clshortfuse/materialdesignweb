  "compilerOptions": {
    "checkJs": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "strict": false,
    "target": "esnext",
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "**/node_modules/*",
    ".history/*"
  ]
}