<div <%~ Object.entries({
  'class': 'androidstatusbar mdw-theme',
  'mdw-surface': it?.surface,
  'mdw-light': it?.light,
  'mdw-dark': it?.dark
  }).map(([key, value]) => {
    if (value === true) return key;
    if (value === false || value == null) return false;
    return `${key}="${value}"`
  }).filter(v=>v).join(' ')-%>>
  <div>12:30</div>
  <i class="material-icons" style="letter-spacing:2px;padding:0 2px;">&#xE1BA;&#xE1C8;&#xE1A4;</i>
</div>
