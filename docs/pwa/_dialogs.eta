<% var T = '../../templates/index.eta' %>
<div class="mdw-dialog" id="dialog-alert" mdw-width-units="5">
  <div class="mdw-dialog__popup mdw-theme" mdw-surface="card">
    <div class="mdw-dialog__title">Alert Dialog</div>
    <div class="mdw-dialog__body mdw-theme" mdw-ink="medium">Alert dialog body text.</div>
    <div class="mdw-dialog__button-area">
      <%~ includeFile(T, { button: { ink:'secondary', attributes:{'mdw-autofocus':true}, text:'OK' }}) %>
    </div>
  </div>
</div>
<div class="mdw-dialog" id="dialog-confirm" mdw-width-units="5">
  <div class="mdw-dialog__popup mdw-theme" mdw-surface="card">
    <div class="mdw-dialog__title">Confirm Dialog</div>
    <div class="mdw-dialog__body mdw-theme" mdw-ink="medium">Confirm dialog body text.</div>
    <div class="mdw-dialog__button-area">
      <%~ includeFile(T, { button: { ink:'secondary', attributes:{'mdw-autofocus':true}, text:'Yes' }}) %>
      <%~ includeFile(T, { button: { ink:'secondary', text:'No' }}) %>
    </div>
  </div>
</div>
<div class="mdw-dialog" id="dialog-autohide-options">
  <div class="mdw-dialog__popup mdw-theme" mdw-surface="card">
    <div class="mdw-dialog__header">
      <div class="mdw-dialog__title">App Bar Auto Hide</div>
    </div>
    <ul class="mdw-dialog__scrolling mdw-list mdw-selection__radio-group" role="radiogroup">
      <li class="mdw-list__item">
        <a class="mdw-list__content mdw-selection" aria-checked="false" data-name="appbar-autohide" data-value="mobile">
          <div class="mdw-list__text mdw-selection__label">
            <div class="mdw-list__text-line">Mobile Only</div>
            <div class="mdw-list__text-line mdw-theme" mdw-ink="medium">[mdw-autohide="mobile"]</div>
          </div>
          <div class="mdw-selection__icon mdw-theme" mdw-ink="secondary" mdw-radio></div>
        </a>
      </li>
      <li class="mdw-list__item">
        <a class="mdw-list__content mdw-selection" aria-checked="false" data-name="appbar-autohide" data-value="tablet">
          <div class="mdw-list__text mdw-selection__label">
            <div class="mdw-list__text-line">Tablet Only</div>
            <div class="mdw-list__text-line mdw-theme" mdw-ink="medium">[mdw-autohide="tablet"]</div>
          </div>
          <div class="mdw-selection__icon mdw-theme" mdw-ink="secondary" mdw-radio></div>
        </a>
      </li>
      <li class="mdw-list__item">
        <a class="mdw-list__content mdw-selection" aria-checked="true" data-name="appbar-autohide" data-value="mobile tablet">
          <div class="mdw-list__text mdw-selection__label">
            <div class="mdw-list__text-line">Mobile and Tablet</div>
            <div class="mdw-list__text-line mdw-theme" mdw-ink="medium">[mdw-autohide="mobile tablet"]</div>
          </div>
          <div class="mdw-selection__icon mdw-theme" mdw-ink="secondary" mdw-radio></div>
        </a>
      </li>
      <li class="mdw-list__item">
        <a class="mdw-list__content mdw-selection" aria-checked="false" data-name="appbar-autohide" data-value="">
          <div class="mdw-list__text mdw-selection__label">
            <div class="mdw-list__text-line">All Devices</div>
            <div class="mdw-list__text-line mdw-theme" mdw-ink="medium">[mdw-autohide=""]</div>
          </div>
          <div class="mdw-selection__icon mdw-theme" mdw-ink="secondary" mdw-radio></div>
        </a>
      </li>
    </ul>
    <!-- While it is possible to close on CHECK_EVENT, this would cause accessibility issues for keyboard users-->
    <div class="mdw-dialog__button-area">
      <%~ includeFile(T, { button: { ink:'secondary', attributes:{'mdw-autofocus':true}, text:'Change' }}) %>
      <%~ includeFile(T, { button: { ink:'secondary', text:'Keep' }}) %>
    </div>
  </div>
</div>
