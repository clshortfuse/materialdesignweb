    </div>
  </div>
</div>
<div class="mdw-dialog" id="dialog-fabdesktop-options">
  <div class="mdw-dialog__popup mdw-theme" mdw-surface="card">
    <div class="mdw-dialog__header">
      <div class="mdw-dialog__title">Mobile Fab Option</div>
    </div>
    <ul class="mdw-dialog__scrolling mdw-list mdw-selection__radio-group" role="radiogroup">
      <li class="mdw-list__item">
        <a class="mdw-list__content mdw-selection" aria-checked="true" data-name="fab-desktop" data-value="">
          <div class="mdw-selection__label mdw-list__text">
            <div class="mdw-list__text-line">End</div>
            <div class="mdw-list__text-line mdw-theme" mdw-ink="medium">[fab-desktop=""]</div>
          </div>
          <div class="mdw-selection__icon mdw-theme" mdw-ink="secondary" mdw-radio></div>
        </a>
      </li>
      <li class="mdw-list__item">
        <a class="mdw-list__content mdw-selection" aria-checked="false" data-name="fab-desktop" data-value="appbar">
          <div class="mdw-selection__label mdw-list__text">
            <div class="mdw-list__text-line">App Bar</div>
            <div class="mdw-list__text-line mdw-theme" mdw-ink="medium">[fab-desktop="appbar"]</div>
          </div>
          <div class="mdw-selection__icon mdw-theme" mdw-ink="secondary" mdw-radio></div>
        </a>
      </li>
    </ul>
    <!-- While it is possible to close on CHECK_EVENT, this would cause accessibility issues for keyboard users-->
    <div class="mdw-dialog__button-area">
      <%~ includeFile(T, { button: { ink:'secondary', attributes:{'mdw-autofocus':true}, text:'Change' }}) %>
      <%~ includeFile(T, { button: { ink:'secondary', text:'Keep' }}) %>
    </div>
