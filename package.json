    "stylelint": "^13.6.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
