{
  "name": "@shortfuse/materialdesignweb",
  "version": "0.7.1-5",
  "description": "Material Design for Web",
  "type": "module",
  "scripts": {
    "publish": "node build/esbuild.js index.js --outdir=dist --minify --metafile --production",
    "predocs": "node bin/generate-css.js --custom=yellow,orange:orange,green:0f0,alias:aqua | npx esbuild --minify --loader=css > docs/theme.css",
    "predocs-alt": "node bin/generate-css.js --color=a46750 --custom=yellow,orange:orange,green:0f0,alias:aqua | npx esbuild --minify --loader=css > docs/alt-theme.css",
    "docs": "node build/esbuild.js docs/demo.js --outdir=docs --minify --metafile",
    "watch": "node build/esbuild.js docs/demo.js --outdir=docs --watch --metafile",
    "serve": "node build/esbuild.js docs/demo.js --outdir=docs --watch --metafile --serve --live",
    "pretest": "eslint --ignore-path .gitignore",
    "test": "c8 tap --no-coverage"
  },
  "browser": "dist/index.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clshortfuse/materialdesignweb.git"
  },
  "keywords": [
    "material",
    "design",
    "web",
    "materialyou",
    "md3",
    "web-components"
  ],
  "files": [
    "/bin",
    "/dist",
    "/components",
    "/core",
    "/theming",
    "/mixins",
    "/utils"
  ],
  "exports": {
    "/components": "./components",
    "/core": "./core",
    "/theming": "./theming",
    "/mixins": "./mixins",
    "/utils": "./utils"
  },
  "author": "Carlos Lopez Jr. <clshortfuse@gmail.com> (https://shortfuse.org/)",
  "license": "ISC",
  "sideEffects": [
    "./polyfills.js",
    "./components/*.js",
    "./utils/metadata.js",
    "./theming/loader.js",
    "./docs/demo/*.js"
  ],
  "bugs": {
    "url": "https://github.com/clshortfuse/materialdesignweb/issues"
  },
  "homepage": "https://github.com/clshortfuse/materialdesignweb#readme",
  "devDependencies": {
    "@types/html-minifier-terser": "^7.0.0",
    "@types/tap": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "acorn": "^8.8.2",
    "acorn-walk": "^8.2.0",
    "browserslist-to-esbuild": "^1.1.1",
    "c8": "^7.12.0",
    "element-internals-polyfill": "^1.1.11",
    "esbuild": "^0.17.12",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.3",
    "eslint-plugin-sort-class-members": "^1.15.2",
    "eslint-plugin-unicorn": "^46.0.0",
    "html-minifier-terser": "^7.1.0",
    "http-server": "^14.1.1",
    "magic-string": "^0.30.0",
    "postcss-styled": "^0.34.0",
    "postcss-styled-syntax": "^0.4.0",
    "stylelint": "^15.2.0",
    "stylelint-no-unsupported-browser-features": "github:clshortfuse/stylelint-no-unsupported-browser-features#0a51157",
    "stylelint-order": "^6.0.2",
    "stylelint-use-logical-spec": "^5.0.0",
    "tap": "^16.3.0",
    "typescript": "^5.0.2",
    "webdriverio": "^7.23.0"
  }
}
