  },
  "homepage": "https://github.com/clshortfuse/materialdesignweb#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-loader": "^8.1.0",
    "clean-css": "^4.2.3",
    "clean-css-loader": "^2.0.0",
    "css-loader": "^3.6.0",
